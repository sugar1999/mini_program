{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue",
			"fui-(.*)": "firstui-uni/firstui/fui-$1/fui-$1.vue",
			"^s-(.*)": "@/uni_modules/s-ui/components/s-$1/s-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页
		{
			"path": "pages/index/tabbar",
			"style": {
				"navigationBarTitleText": "",
				"navigationStyle": "custom"
			}
		}
	],

	"subPackages": [
		{
			"root":"pages/news",
			"pages": [{
				"path":"news",
				"style":{
					"navigationBarTitleText": "旅游详情"
				}
			}]
		},
		{
			"root": "page_hot",
			"pages": [{
				"path": "hot_index/hot_index",
				"style": {
					"navigationStyle": "custom"
				}
			}]
		},
		{
			"root": "page_secure",
			"pages": [{
					"path": "secure_index/secure_index",
					"style": {
						"navigationStyle": "custom"
					}
				},
				{
					"path": "secure_item/secure_inquiry",
					"style": {
						"navigationStyle": "custom"
					}
				}, 
				{
					"path": "secure_item/secure_setout",
					"style": {
						"navigationStyle": "custom"
					}
				},
				{
					"path": "secure_item/secure_anquan",
					"style": {
						"navigationStyle": "custom"
					}
				},
